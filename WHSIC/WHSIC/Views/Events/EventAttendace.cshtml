@model IEnumerable<WHSIC.Data.EventGuest>
@{
    ViewBag.Title = "EventAttendace";
    Layout = "~/Views/Secretary/_DashBoard.cshtml";
}
<h3>Number of attendence @ViewBag.Attendencenum</h3>
<h2>Attendance List</h2>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Guests.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Replydate)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Guests.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Replydate)
            </td>
        </tr>
    }

</table>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>