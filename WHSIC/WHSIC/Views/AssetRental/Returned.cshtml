@model WHSIC.Model.Asset_RentalViewModel

@{
    ViewBag.Title = "Returned";
}

<h2>Returned</h2>
<div class="well">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-horizontal">
            <h4>Asset Rental</h4>
            <hr />
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            @Html.HiddenFor(model => model.Rental_code)
            @Html.HiddenFor(model => model.Asset_Code)
            <div class="form-group">
                @Html.Label("Quantity to be Returned", htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-3">
                    <input type="text" name="txtremqyt" value="@ViewBag.remQty" readonly="readonly" class="form-control" />
                </div>
            </div>

            <div class="form-group">
                @Html.LabelFor(model => model.quantity, htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-3">
                    @Html.EditorFor(model => model.quantity, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.quantity, "", new { @class = "text-danger" })
                </div>
            </div>
            @*<div hidden>
                     <input type="text" name="txtrqyt" value="@ViewBag.rqty" hidden />
                    <input type="text" name="txthqyt" value="@ViewBag.hqty" hidden />
                </div>*@
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>
            </div>
        </div>
    }

    <div>
        @Html.ActionLink("Back to List", "Index")
    </div>
    @if (ViewBag.feed != null)
    {
        <script type="text/javascript">
            alert("@ViewBag.feed")
        </script>
    }

    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }
</div>

